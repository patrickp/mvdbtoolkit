SUBROUTINE MVDBTOOLKIT.SWAP(V,F,T)

* MVDBTOOLKIT.SWAP

* PLATFORM INDEPENDENT SWAP COMMAND
CALL MVDBTOOLKIT.WPLATFORM(PLATFORMOBJ)

CALL MVDBTOOLKIT.WOBJ(PLATFORMOBJ,"GET","mvtype",MV.TYPE,"",RERR)

BEGIN CASE
    CASE MV.TYPE='UNIVERSE'
        CALL MVDBTOOLKIT.SENTENCE.UNIVERSE(V,F,T)
    CASE 1
        CALL MVDBTOOLKIT.SENTENCE.DEFAULT(V,F,T)
END CASE

RETURN

