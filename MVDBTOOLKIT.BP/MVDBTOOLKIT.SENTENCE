SUBROUTINE MVDBTOOLKIT.SENTENCE(V)

* MVDBTOOLKIT.SWAP

* PLATFORM INDEPENDENT SWAP COMMAND
CALL MVDBTOOLKIT.WPLATFORM(PLATFORMOBJ)

CALL MVDBTOOLKIT.WOBJ(PLATFORMOBJ,"GET","mvtype",MV.TYPE,"",RERR)

BEGIN CASE
    CASE MV.TYPE='UNIDATA'
        CALL MVDBTOOLKIT.SENTENCE.UNIDATA(V)
    CASE 1
        CALL MVDBTOOLKIT.SENTENCE.DEFAULT(V)
END CASE

RETURN

