SUBROUTINE MVDBTOOLKIT.SWAP.UNIVERSE(V,F,T)

IF F=T THEN RETURN
IF F='' THEN RETURN

* Let's do this manually since Universe Exchange does NOT work
CNTR=0
LOOP
   CNTR+=1
   POS=INDEX(V,F,1)
UNTIL NOT(POS) OR CNTR > 100000 DO

   PRE.STRING=V[1,POS-1]
   AFT.STRING=V[POS+LEN(F),LEN(V)]
   V=PRE.STRING:T:AFT.STRING

REPEAT

RETURN

